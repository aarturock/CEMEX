@model CEMEX.Web.Models.ViewModelRolCrear

@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/_LayoutPage.cshtml";
}

<div id="page-content">
    <h1 class="page-header"><i class="modulo-configuracion"></i> Configuracion</h1>

    <section id="widget-grid">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">

                <!--================= Inicia Widget (cada widget requiere un ID único) ==================-->
                <div class="jarviswidget jarviswidget-color-red" id="wid-id-0" data-widget-deletebutton="false">
                    <header> <h2>Alta de Roles</h2> </header>
                    <div>
                        <div class="jarviswidget-editbox"> <input class="form-control" type="text"> </div>

                        <div class="widget-body">

                            <div class="expediente-tabs">
                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs" role="tablist">
                                    <li role="presentation" class="active">
                                        <a href="#info" aria-controls="informacion general" role="tab" data-toggle="tab">
                                            Información General
                                        </a>
                                    </li>
                                    <li role="presentation">
                                        <a href="#funcionalidadesWeb" aria-controls="funcionalidades" role="tab" data-toggle="tab">
                                            Funcionalidades Web
                                        </a>
                                    </li>
                                    <li role="presentation">
                                        <a href="#funcionalidadesApp" aria-controls="funcionalidades" role="tab" data-toggle="tab">
                                            Funcionalidades Móvil
                                        </a>
                                    </li>
                                </ul>
                                <!-- Tab panes -->
                                <div class="tab-content">
                                    <div role="tabpanel" class="tab-pane active fade in" id="info">
                                        <form role="form">
                                            <fieldset>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group">
                                                            <legend>Información General</legend>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-12">Nombre del Rol</label>
                                                            <input type="text" class="form-control col-sm-12" placeholder="Nombre del Rol">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-8">
                                                        <div class="form-group">
                                                            <label class="control-label col-sm-12">Descripción del Rol</label>
                                                            <textarea class="form-control col-sm-12" placeholder="Descripción del Rol" rows="7" cols="15"></textarea>
                                                        </div>
                                                    </div>
                                                </div>

                                                <hr>

                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group">
                                                            @for (int indexJerarquia = 0; indexJerarquia < Model.Jerarquias.Count; indexJerarquia++)
                                                            {
                                                                <div class="col-xs-6 col-sm-6 col-md-3 col-lg-2">
                                                                    <div class="radio">
                                                                        <label>
                                                                            @Html.RadioButtonFor(m => m.IdJerarquia, @Model.Jerarquias[indexJerarquia].Id)
                                                                            @Model.Jerarquias[indexJerarquia].Nombre
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            }
                                                        </div>
                                                    </div>
                                                </div>

                                                <hr>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group pull-right">
                                                            <label class="control-label">Status del registro</label>
                                                            <div class="make-switch" data-text-label="Status" data-on-label="<i class='gi-ok_2'></i> &nbsp; Activo" data-off-label="<i class='gi-remove_2'></i> &nbsp; Inactivo" data-on="success" data-off="danger">
                                                                <input class="form-control" type="checkbox" checked="checked">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </form>
                                    </div>
                                    <div role="tabpanel" class="tab-pane fade" id="funcionalidadesWeb">
                                        <form role="form">
                                            <fieldset>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group">
                                                            <legend>Funcionalidades Web</legend>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="alert alert-info" role="alert">
                                                            Selecciona un ícono para autorizar el acceso a esa funcionalidad. O marca el nombre de la funcionalidad para seleccionarlos todos.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        @for (int indexModuloWeb = 0; indexModuloWeb < Model.ModulosWeb.Count; indexModuloWeb++)
                                                        {
                                                            if (Model.ModulosWeb[indexModuloWeb].Paginas.Count > 0)
                                                            {
                                                                <div id="funcionalidadesWeb-panel-@indexModuloWeb" class="panel-group">
                                                                    <div class="panel panel-default">
                                                                        <div class="panel-heading">
                                                                            <a href="#funcionalidadesWeb-box-@indexModuloWeb" data-parent="#funcionalidadesWeb-panel-@indexModuloWeb" data-toggle="collapse">
                                                                                <h3 class="panel-title"> @Model.ModulosWeb[indexModuloWeb].Nombre <i class="fa fa-caret-down"></i></h3>
                                                                            </a>
                                                                        </div>
                                                                        <div id="funcionalidadesWeb-box-@indexModuloWeb" class="panel-collapse fade collapse">
                                                                            <div class="panel-body">
                                                                                <div class="row">
                                                                                    <div class="col-md-4">
                                                                                        <div class="form-group">
                                                                                            <div class="checkbox">
                                                                                                <label> <input type="checkbox" role="button"> <h3>Seleccionar todos</h3></label>
                                                                                            </div>
                                                                                        </div>
                                                                                        @for (int indexPagina = 0; indexPagina < Model.ModulosWeb[indexModuloWeb].Paginas.Count; indexPagina++)
                                                                                        {
                                                                                            <div class="form-group">
                                                                                                <div class="checkbox">
                                                                                                    <label> <input type="checkbox" role="button"> <p> @Model.ModulosWeb[indexModuloWeb].Paginas[indexPagina].Nombre</p></label>
                                                                                                </div>
                                                                                            </div>
                                                                                        }
                                                                                    </div>
                                                                                    <div class="col-md-8">
                                                                                        <div class="form-group">
                                                                                            <div class="btn-select" data-toggle="buttons">
                                                                                                @for (int indexPermiso = 0; indexPermiso < Model.Permisos.Count; indexPermiso++)
                                                                                                {
                                                                                                    <span><input type="checkbox" value=""><p> @Model.Permisos[indexPermiso].Nombre</p></span>
                                                                                                }
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            }
                                                        }





